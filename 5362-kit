--\\5362 Kit//--
--\\For Level 3 Executor//--

local kit5362 = Instance.new("ScreenGui")
local mainFrame = Instance.new("Frame")
local UICorner = Instance.new("UICorner")
local Buttons = Instance.new("Folder")
local HitboxButtons = Instance.new("Folder")
local _25 = Instance.new("TextButton")
local UICorner_2 = Instance.new("UICorner")
local _50 = Instance.new("TextButton")
local UICorner_3 = Instance.new("UICorner")
local _75 = Instance.new("TextButton")
local UICorner_4 = Instance.new("UICorner")
local _10 = Instance.new("TextButton")
local UICorner_5 = Instance.new("UICorner")
local _5 = Instance.new("TextButton")
local UICorner_6 = Instance.new("UICorner")
local HitboxTransparency = Instance.new("TextButton")
local UICorner_7 = Instance.new("UICorner")
local Close = Instance.new("TextButton")
local UICorner_8 = Instance.new("UICorner")
local HitboxDistance = Instance.new("TextButton")
local UICorner_9 = Instance.new("UICorner")
local Labels = Instance.new("Folder")
local HitboxInfo = Instance.new("TextLabel")
local BuildTip = Instance.new("TextLabel")
local Title = Instance.new("TextLabel")
local version = Instance.new("TextLabel")
local HideWarn = Instance.new("TextLabel")
local dc = Instance.new("TextLabel")
local dc2 = Instance.new("TextLabel")
local cre2 = Instance.new("TextLabel")
local UIAspectRatioConstraint = Instance.new("UIAspectRatioConstraint")
local UIS = game:GetService("UserInputService")

-- Create and set up the ScreenGui
kit5362.Name = "kit5362"
kit5362.Parent = game.Players.LocalPlayer:WaitForChild("PlayerGui")
kit5362.ZIndexBehavior = Enum.ZIndexBehavior.Sibling
kit5362.ResetOnSpawn = false

-- Create and set up the mainFrame
mainFrame.Name = "mainFrame"
mainFrame.Parent = kit5362
mainFrame.BackgroundColor3 = Color3.fromRGB(0, 0, 0)  -- Black background
mainFrame.Position = UDim2.new(0, 424, 0, 116)
mainFrame.Size = UDim2.new(0, 770, 0, 294)
mainFrame.Visible = true -- Start visible for debugging

UICorner.Parent = mainFrame

-- Create an icon for toggling the mainFrame
local iconButton = Instance.new("ImageButton")
iconButton.Parent = kit5362
iconButton.Position = UDim2.new(0, 50, 0, 50)
iconButton.Size = UDim2.new(0, 50, 0, 50)
iconButton.Image = "rbxassetid://6023426915"  -- Example image for the icon
iconButton.BackgroundTransparency = 1

-- Make the icon draggable
local dragToggle = nil
local dragStart = nil
local startPos = nil

local function updateDrag(input)
    local delta = input.Position - dragStart
    iconButton.Position = UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y)
end

iconButton.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragToggle = true
        dragStart = input.Position
        startPos = iconButton.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragToggle = false
            end
        end)
    end
end)

iconButton.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        if dragToggle then
            updateDrag(input)
        end
    end
end)

-- Toggle the menu when iconButton is clicked
iconButton.MouseButton1Click:Connect(function()
    mainFrame.Visible = not mainFrame.Visible
end)

-- Make the mainFrame draggable
local dragMain = false
local dragInputMain
local dragStartMain
local startPosMain

mainFrame.InputBegan:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
        dragMain = true
        dragStartMain = input.Position
        startPosMain = mainFrame.Position
        input.Changed:Connect(function()
            if input.UserInputState == Enum.UserInputState.End then
                dragMain = false
            end
        end)
    end
end)

mainFrame.InputChanged:Connect(function(input)
    if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
        dragInputMain = input
    end
end)

UIS.InputChanged:Connect(function(input)
    if input == dragInputMain and dragMain then
        local delta = input.Position - dragStartMain
        mainFrame.Position = UDim2.new(startPosMain.X.Scale, startPosMain.X.Offset + delta.X, startPosMain.Y.Scale, startPosMain.Y.Offset + delta.Y)
    end
end)

-- Sample Button to ensure visibility
local sampleButton = Instance.new("TextButton")
sampleButton.Parent = mainFrame
sampleButton.Position = UDim2.new(0, 10, 0, 10)
sampleButton.Size = UDim2.new(0, 100, 0, 50)
sampleButton.Text = "Sample Button"
sampleButton.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red background for visibility

-- Add remaining UI elements and buttons here
-- You should create and configure Buttons, Labels, etc., as needed.

-- Example for adding a close button
Close.Name = "Close"
Close.Parent = mainFrame
Close.Position = UDim2.new(0, 710, 0, 10)
Close.Size = UDim2.new(0, 50, 0, 50)
Close.Text = "X"
Close.BackgroundColor3 = Color3.fromRGB(255, 0, 0) -- Red background for visibility
Close.TextColor3 = Color3.fromRGB(255, 255, 255) -- White text

Close.MouseButton1Click:Connect(function()
    mainFrame.Visible = false
end)
